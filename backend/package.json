{
  "name": "@rdjcustoms/backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "vitest --config vitest.config.unit.js",
    "test:watch": "vitest --config vitest.config.unit.js --watch",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --config vitest.config.unit.js",
    "test:unit:watch": "vitest --config vitest.config.unit.js --watch",
    "test:models": "vitest --config vitest.model.config.js",
    "test:models:watch": "vitest --config vitest.model.config.js --watch",
    "test:integration": "vitest --config vitest.integration.config.js",
    "test:integration:watch": "vitest --config vitest.integration.config.js --watch",
    "test:integration:docker": "vitest --config vitest.integration.docker.config.js",
    "test:integration:docker:watch": "vitest --config vitest.integration.docker.config.js --watch",
    "test:integration:docker:shared": "KEEP_DOCKER_RUNNING=true vitest --config vitest.integration.docker-shared.config.js",
    "test:integration:docker:cleanup": "docker stop rdjcustoms-test-shared && docker rm rdjcustoms-test-shared",
    "docker:test:up": "docker-compose -f docker-compose.test.yml up -d",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down",
    "docker:test:logs": "docker-compose -f docker-compose.test.yml logs -f",
    "test:e2e": "vitest --config vitest.config.e2e.js",
    "test:e2e:watch": "vitest --config vitest.config.e2e.js --watch",
    "test:performance": "vitest --config vitest.config.performance.js",
    "test:performance:watch": "vitest --config vitest.config.performance.js --watch",
    "test:security": "vitest --config vitest.config.security.js",
    "test:security:watch": "vitest --config vitest.config.security.js --watch",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:unit && npm run test:integration",
    "test:coverage": "vitest --config vitest.config.unit.js --coverage",
    "test:coverage:integration": "vitest --config vitest.integration.config.js --coverage",
    "test:coverage:all": "vitest --config vitest.config.unit.js --coverage && vitest --config vitest.integration.config.js --coverage",
    "test:playwright": "playwright test",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:report": "playwright show-report",
    "test:playwright:install": "playwright install",
    "test:playwright:payments": "playwright test --grep=\"Payment Flows\"",
    "test:playwright:users": "playwright test --grep=\"User Management\"",
    "test:legacy:jest": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:legacy:unit": "node --experimental-vm-modules node_modules/.bin/jest --config jest.config.unit.js",
    "test:legacy:integration": "node --experimental-vm-modules node_modules/.bin/jest --testPathPattern=\"integration\"",
    "test:legacy:services": "node --experimental-vm-modules node_modules/.bin/jest src/services/**/*.test.js",
    "test:legacy:routes": "node --experimental-vm-modules node_modules/.bin/jest src/routes/**/*.test.js",
    "test:legacy:performance": "node --experimental-vm-modules node_modules/.bin/jest --testPathPattern=\"performance\"",
    "test:legacy:security": "node --experimental-vm-modules node_modules/.bin/jest --testPathPattern=\"security\"",
    "test:legacy:load": "node --experimental-vm-modules node_modules/.bin/jest --testPathPattern=\"load\"",
    "test:legacy:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "test:legacy:verbose": "node --experimental-vm-modules node_modules/.bin/jest --verbose",
    "seed": "node src/scripts/importEtsy.js",
    "create-admin": "node src/scripts/createAdminUser.js",
    "import-etsy": "node src/scripts/importEtsy.js",
    "remove-categories": "node src/scripts/removeCategories.js",
    "test:email": "node src/scripts/testEmailService.js",
    "test:email:simple": "node src/scripts/testEmailServiceSimple.js",
    "test:ses": "node src/scripts/testEmailServiceSES.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
