    "@sentry/node": "^9.29.0",
    "@sentry/profiling-node": "^9.29.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "isomorphic-dompurify": "^2.25.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "newrelic": "^12.21.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.3",
    "qrcode.react": "^4.2.0",
    "sharp": "^0.34.2",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.0-beta.3",
    "@playwright/test": "^1.53.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "artillery": "^2.0.23",
    "c8": "^10.1.3",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "msw": "^2.10.2",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "vitest": "^3.2.4"
  }
}
